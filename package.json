{
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --prod",
    "test": "ng test --watch",
    "test:cli": "ng test --no-watch",
    "prettier": "prettier --write 'src/**/*.*'",
    "tslint": "tslint './src/**/*.ts' --fix",
    "lint": "npm run prettier && npm run tslint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --only hosting,firestore:rules",
    "component": "func() { ng g component component/\"$1\"/\"$2\" && ng g service component/\"$1\"/\"$2\"/\"$2\" && npm run lint; }; func",
    "page": "func() { ng g component component/page/\"$1\" && ng g af component/page/\"$1\"/\"$1\" --spec --plain && npm run lint; }; func",
    "modal": "func() { ng g component component/modal/\"$1\" && ng g af component/modal/\"$1\"/\"$1\" --spec --plain && npm run lint; }; func"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@datorama/akita": "^4.23.0",
    "@datorama/akita-ng-entity-service": "^1.0.0",
    "@fortawesome/angular-fontawesome": "~0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/pro-duotone-svg-icons": "^5.12.1",
    "@fortawesome/pro-solid-svg-icons": "^5.12.1",
    "@sentry/angular": "^5.27.0",
    "@sentry/tracing": "^5.27.0",
    "firebase": "^8.2.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@angular/language-service": "~11.0.9",
    "@datorama/akita-ng-router-store": "^3.1.3",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
    "firebase-tools": "^9.2.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "prettier": "^2.0.5"
  }
}
